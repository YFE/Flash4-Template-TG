oss ='./';
vs = '?v=123';

bgcolor = #000;
loadingColor = #fff;

vendor(prop, args)
    -webkit-{prop} args
    {prop} args

@import "reset";
@import "font";

html, body {
    background: bgcolor;
    width 100%
    height 100%
    overflow: hidden;
    word-break: break-all;
}
input::-webkit-input-placeholder {
    color: rgba(0, 0, 0, 0.3);
}

//rem 
